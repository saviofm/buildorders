COLUMN TABLE productstrainning_Products (
  ID NVARCHAR(36) NOT NULL,
  createdAt TIMESTAMP,
  createdBy NVARCHAR(255),
  modifiedAt TIMESTAMP,
  modifiedBy NVARCHAR(255),
  EAN NVARCHAR(5000),
  productName NVARCHAR(5000),
  productDescription NVARCHAR(5000),
  brand NVARCHAR(5000),
  obs NCLOB,
  image NCLOB,
  PRIMARY KEY(ID)
)